#ifndef IMPUESTO_H_
#define IMPUESTO_H_
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "general.h"
#include "pedido.h"

#define FALSE 0
#define TRUE 1


typedef struct
{
	int idPedido;
	float impuesto;
	char zonaContinental[51];
	int isTaxed;

}eImpuesto;

int inicializarImpuesto(eImpuesto list[], int TAMIMPUESTO);
int idImpuesto(ePedido listaPedido[], int TAMPEDIDO, eImpuesto listaImpuesto[], int TAMIMPUESTO);

#endif /* IMPUESTO_H_ */
